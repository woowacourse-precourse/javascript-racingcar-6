{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:jsdoc/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": ["off"],
    // 들여쓰기 깊이 제한
    "max-depth": ["error", 2],
    "import/prefer-default-export": ["off"]
  }
}
