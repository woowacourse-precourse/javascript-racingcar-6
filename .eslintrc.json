{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "lines-between-class-members": [
      "error",
      {
        "enforce": [{ "blankLine": "never", "prev": "field", "next": "field" }]
      }
    ]
  }
}
